<?xml version="1.0" encoding="UTF-8" ?>
<templates templatePath="/template/">
    <template type="sqlMap">
        <templateName>/ibatis/sqlmap/ibatis_sqlmap.vm</templateName>
        <targetFileName replacement="{desc}" replaceWith="desc">/ibatis/sqlMap/{desc}_SqlMap.xml</targetFileName>
    </template>
    <template type="model">
        <templateName>model/model.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/ibatis/model/{className}.java</targetFileName>
    </template>
    <template type="query">
        <templateName>query/query.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/ibatis/query/{className}Query.java</targetFileName>
    </template>
    <template type="dao">
        <templateName>dao/dao.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/ibatis/dao/{className}DAO.java</targetFileName>
    </template>
    <template type="daoImpl">
        <templateName>dao/impl/daoImpl.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/ibatis/dao/impl/{className}DAOImpl.java</targetFileName>
    </template>
    <template type="service">
        <templateName>service/service.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/ibatis/service/{className}Service.java</targetFileName>
    </template>
    <template type="serviceImpl">
        <templateName>service/impl/serviceImpl.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/ibatis/service/impl/{className}ServiceImpl.java</targetFileName>
    </template>
</templates>