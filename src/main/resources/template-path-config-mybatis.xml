<?xml version="1.0" encoding="UTF-8" ?>
<templates templatePath="/template/">
    <template type="sqlMap">
        <templateName>mybatis/sqlmap/mybatis_sqlmap.vm</templateName>
        <targetFileName replacement="{desc}" replaceWith="desc">/sqlMap/{desc}_SqlMap.xml</targetFileName>
    </template>
    <template type="model">
        <templateName>mybatis/model/model.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/entity/{className}.java</targetFileName>
    </template>
    <template type="query">
        <templateName>mybatis/query/query.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/query/{className}Query.java</targetFileName>
    </template>
    <template type="dao">
        <templateName>mybatis/dao/dao.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/dao/{className}Dao.java</targetFileName>
    </template>
    <template type="mapper">
        <templateName>mybatis/dao/mapper/mapper.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/dao/mapper/{className}Mapper.java</targetFileName>
    </template>
    <template type="daoImpl">
        <templateName>mybatis/dao/impl/daoImpl.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/dao/impl/{className}DaoImpl.java</targetFileName>
    </template>
    <template type="service">
        <templateName>mybatis/service/service.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/service/{className}Service.java</targetFileName>
    </template>
    <template type="serviceImpl">
        <templateName>mybatis/service/impl/serviceImpl.vm</templateName>
        <targetFileName replacement="{className}" replaceWith="className">/service/impl/{className}ServiceImpl.java</targetFileName>
    </template>
</templates>